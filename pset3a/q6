dna=''

a_count=0
t_count=0
g_count=0
c_count=0

for letter in dna:
	if letter == 'A' or letter =='a':
		a_count +=1
	elif letter =='T' or letter == 't':
		t_count+=1
	elif letter == 'G' or letter == 'g':
		g_count+=1
	elif letter == 'C' or letter == 'c':
		c_count+=1
g_count=str(g_count)
c_count=str(c_count)
print("The number of A's is:",a_count)
print("The number of T's is:",t_count)
print("The number of G's is: " + g_count)
print("The number of C's is: " + c_count)


